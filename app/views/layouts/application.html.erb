<!DOCTYPE html>
<html>
  <head>
    
    <title><%= full_title(yield(:title)) %></title>
    
    <%= render 'layouts/stylesheets' %>
    
    <%= javascript_include_tag "application" %>
    
    <%= javascript_include_tag "corner" %>
    
    <%= stylesheet_link_tag 'roundabout', media: 'all' %>
    <%= javascript_include_tag "jquery.roundabout" %>
    <%= javascript_include_tag "jquery.roundabout-shapes.min" %>


    <%= stylesheet_link_tag 'hot-sneaks/jquery-ui-1.8.17.custom', media: 'all' %>
    <%= javascript_include_tag "jquery-ui-1.8.17.custom.min" %>
    
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container">
      <%= render 'layouts/header' %>
      <%= yield :main %>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>